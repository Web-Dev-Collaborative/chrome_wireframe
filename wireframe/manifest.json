{
    "name": "Wireframe",
    "manifest_version": 2,
    "version": "0.2",
    "description": "Colors the divs of the pages, wireframe up!",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "homepage_url": "http://github.com/ldong/chrome_wireframe",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}
